<h1>All Students:</h1>
<% @students.each.with_index(1) do |s, index| %>
    <p>
        <%=  index.to_s + ". " + s.first_name + " " + s.last_name %>
        <span><%= link_to "Edit", edit_student_path(s) %> </span>
        <span><%= link_to "Delete", student_path(s), method: "delete" %> </span>
    </p>
<% end %>

<p><%= link_to "Add student", new_student_path %></p>
